{
  "name": "example-node-server",
  "version": "0.1.0",
  "description": "Example Node Server w/ Babel",
  "main": "lib/index.js",
  "scripts": {
    "start": "nodemon lib/index.js --exec babel-node --presets es2015,stage-2",
