  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.11.0",
    "babel-register": "6.26.0",
    "mocha": "3.0.1",
    "nodemon": "1.19.1"
  },
  "dependencies": {
    "bcrypt": "3.0.6",
    "express": "4.16.2",
    "jsonwebtoken": "8.1.1",
    "jwt-simple": "0.5.6",
    "moment": "2.22.2",
    "morgan": "1.9.1",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "7.4.1",
    "rimraf": "2.6.2",
    "sequelize": "4.31.2",
